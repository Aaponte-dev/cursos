{
  "name": "backendnode",
  "version": "0.1.0",
  "description": "Creacion de chat con webSocket",
  "main": "server.js",
  "scripts": {
    "start": "tsc && node dist/src/app.js",
    "dev": "npx nodemon --inspect src/app.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\" ",
    "lint:fix": "eslint \"*/**/*.{js,ts,tsx}\" --fix",
    "prepare": "husky install"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "archivos/*",
      "lecciones/*"
    ]
  },
  "keywords": [
    "nodeJs",
    "expressJs",
    "typeScript"
  ],
  "author": "Angel Aponte <aponte_14leonardo@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@typegoose/typegoose": "^11.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.36",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/qs": "^6.9.8",
    "@types/serve-static": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}